{
  "name": "cypress-workshop",
  "version": "1.0.0",
  "description": "Cypress.io workshop",
  "main": "index.js",
  "scripts": {
    "dev": "cypress open",
    "test": "cypress run",
    "lint": "eslint cypress/",
    "stop-only": "stop-only --folder cypress/integration --skip node_modules"
  },
  "husky": {
